{
  "name": "multi-tenant-github-dashboard",
  "version": "1.0.0",
  "description": "A serverless multi-tenant GitHub dashboard for tracking commits and contributions across repositories",
  "type": "module",
  "scripts": {
    "dev": "python3 -m http.server 8080",
    "dev:worker": "cd workers && wrangler dev",
    "deploy:worker": "cd workers && wrangler deploy",
    "deploy": "vercel deploy --prod",
    "preview": "vercel deploy"
  },
